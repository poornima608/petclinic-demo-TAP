pipeline {
  agent any
  stages{
    stage ('Create AWS Stack') {
      steps {
        echo "Creating AWS CloudFormation Stack"
       withCredentials([[$class: 'AmazonWebServicesCredentialsBinding', accessKeyVariable: 'AWS_ACCESS_KEY_ID', credentialsId: 'aws-key', secretKeyVariable: 'AWS_SECRET_ACCESS_KEY']]) {
    // some block

          sh 'aws cloudformation create-stack --stack-name petclinic --template-body file://aws-template-petclinic.yaml --region us-east-1 --parameters  ParameterKey=SSHKey,ParameterValue=AWS_EC2_Project ParameterKey=ELBVPC,ParameterValue=vpc-47a7483a ParameterKey=ELBSubnet,ParameterValue="subnet-e1a9caac\\,subnet-Oc7d9a3d"'
          }
          }
          }
          }
